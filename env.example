# Configurações do Sistema de Monitoramento Rocks
# Copie este arquivo para .env e ajuste as configurações

# API Configuration
ROCKS_API_URL=https://wretched-casket-7vrr9w7rv5q5fxjp5-8000.app.github.dev
ROCKS_API_TIMEOUT=10
ROCKS_API_RETRY_ATTEMPTS=3

# Logging Configuration
ROCKS_LOG_LEVEL=INFO
ROCKS_LOG_FILE=data/rocks_monitoramento.log

# Environment Configuration
ROCKS_ENV=development  # development, testing, production
ROCKS_DEBUG=true
ROCKS_AUTO_RELOAD=true

# Monitoring Configuration
ROCKS_MONITORING_INTERVAL=5
ROCKS_MONITORING_ENABLED=true

# UI Configuration
ROCKS_UI_THEME=dark
ROCKS_UI_LANGUAGE=pt_BR

